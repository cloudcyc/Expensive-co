    <div class="w3-main" style="margin-left:250px">
    <div style="background-color:#292929">
      <button class="w3-button w3-xlarge w3-hide-large" onclick="w3_open()" style="color:white">&#9776;</button>
      <div class="w3-container" style="text-align:right">
            <Label Class="text-white">Welcome back,</Label> <asp:Label Class="text-white" ID="AdminName" runat="server" Text="Label"></asp:Label> &nbsp; &nbsp;
      </div>
    </div>

    
    <div id="main-wrapper" data-layout="vertical" data-navbarbg="skin5" data-sidebartype="full" data-sidebar-position="absolute" data-header-position="absolute" data-boxed-layout="full">
