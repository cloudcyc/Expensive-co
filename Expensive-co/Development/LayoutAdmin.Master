    <div class="w3-sidebar w3-bar-block w3-collapse w3-card w3-animate-left" style="width:250px;" id="mySidebar">
    <button class="w3-bar-item w3-button w3-large w3-hide-large" onclick="w3_close()">Close &times;</button>
        <a class="navbar-brand" href="AdminDashboard.aspx">
            <!-- Logo icon -->
            <b class="logo-icon">
                <!-- Dark Logo icon -->
                <img src="../Assets/img/logo.png" style="height:80px">
            </b>
            <!--End Logo icon -->                        
        </a>  
        <a class="w3-bar-item w3-button" href="AdminDashboard.aspx"
            aria-expanded="false">
            <i class="far fa-clock" aria-hidden="true"></i>
            <span class="hide-menu">Dashboard</span>
        </a>
        <a class="w3-bar-item w3-button" href="AdminEditProfile.aspx"
            aria-expanded="false">
            <i class="fa fa-user" aria-hidden="true"></i>
            <span class="hide-menu">Profile</span>
        </a>
        <a class="w3-bar-item w3-button" href="AdminManageUser.aspx"
            aria-expanded="false">
            <i class="fa fa-table" aria-hidden="true"></i>
            <span class="hide-menu">Manage User</span>
        </a>
        <a class="w3-bar-item w3-button" href="ProductList.aspx"
            aria-expanded="false">
            <i class="fa fa-table" aria-hidden="true"></i>
            <span class="hide-menu">Product List</span>
        </a>
        <a class="w3-bar-item w3-button" href="SalesHistory.aspx"
            aria-expanded="false">
            <i class="fa fa-table" aria-hidden="true"></i>
            <span class="hide-menu">Sales History</span>
        </a>
        <a class="w3-bar-item w3-button" href="AdminViewFeedback.aspx"
            aria-expanded="false">
            <i class="fa fa-table" aria-hidden="true"></i>
            <span class="hide-menu">Feedback</span>
        </a>
        <a class="w3-bar-item w3-button" href="Home.aspx"
            aria-expanded="false">
            <i class="fa fa-table" aria-hidden="true"></i>
            <span class="hide-menu">View Store</span>
        </a>
        <a href="Logout.aspx"" class ="btn d-grid btn-danger text-white">Logout</a>
